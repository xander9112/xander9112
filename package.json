{
  "name": "xander9112-blog",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://xander9112.github.io/xander9112",
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "exif-js": "^2.3.0",
    "firebase": "^5.4.2",
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/_assets --include-path ./node_modules src/_assets -o src/_assets",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/_assets --include-path ./node_modules src/_assets -o src/_assets --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@types/firebase": "^3.2.1",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.8",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^7.4.7",
    "@types/redux-logger": "^3.0.6",
    "typescript": "^3.0.3"
  }
}
